Proceso ejemplo2
	// se requiere un algoritmo para una veterianaria
	// que permita tener una variable para
	// el tipo de mazcota, peso y tratamiento
	// determinado por el veterinario. asignar
	//las diferentes variables y mostrar todos los datos
	
	Definir tipodemascota, nombremascota, tratamiento como cadena;
	Definir pesomascota Como Real;
	tipodemascota<- "perro";
	nombremascota <- "zeus";
	tratamiento <- "examen";
	pesomascota <- 5.4;
	Escribir "que tipo de mascota es:", tipodemascota;
	Escribir "cual es el nombre de la mascota:", nombremascota;
	Escribir "cual es el tratamiento:", tratamiento;
	Escribir "cual es el peso de la mascota:", pesomascota;
	
	

	
FinProceso





