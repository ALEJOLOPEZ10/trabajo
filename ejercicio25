Algoritmo ejercicio25
	// Definicion/declaracion
	Definir nota como Real;
	// datos por consola
	Escribir "Ingrese la nota";
	Leer nota;
	Si (nota >= 0 y nota <= 2.9)Entonces
		Escribir "Nota insuficiente: ", nota;
	SiNo
		Si (nota >= 3 y nota <= 4.5)Entonces
			Escribir "Nota suficiente: ", nota;
		SiNo
			Si (nota >= 4.6 y nota <= 5)Entonces
				Escribir "Excelente: ", nota;
			FinSi
		FinSi
	FinSi
FinAlgoritmo
