Proceso ejercicio11
	//definir variables
	Definir A, B, diferencia Como Entero;
	//se ingresa valor
	Escribir "Ingresa el primer numero";
	Leer A;
	Escribir "Ingresa el segundo numero";
	Leer B;
	//proceso
	diferencia <- A - B;
	//se imprime consola
	Escribir "La diferencia entre ambos numeros es: ", diferencia;
FinProceso
